    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: densify linestrings",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "densify",
                "${workspaceFolder}/tests/data/linestrings.json",
                "${workspaceFolder}/out-data/out.json",
                "--max-segment-length", "5000"
            ],
            "preLaunchTask": "clean-output",
            "justMyCode": false
        },
        {
            "name": "Python: densify linestrings - SHP ",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "densify",
                "${workspaceFolder}/tests/data/linestrings.json",
                "${workspaceFolder}/out-data/out.shp"
            ],
            "preLaunchTask": "clean-output-shp",
            "justMyCode": false
        },
        {
            "name": "Python: densify linestrings (GPKG)",
